<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"


    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.testble.MainActivity" >


    <EditText
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:inputType="textPersonName"
        android:text="Play Song"
        android:ems="10"
        android:id="@+id/text"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/chordCsharpdim" />

    <RadioButton
        android:text="down"
        android:layout_width="2dp"
        android:layout_height="2dp"
        android:id="@+id/nothing"
        android:layout_below="@+id/text11"
        android:layout_toEndOf="@+id/keyF" />


    <TextView
        android:text="Key"
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:id="@+id/textView"
        android:layout_alignBottom="@+id/radioButton5"
        android:layout_alignParentStart="true" />

    <SeekBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/reverb1"
        android:max="100"
        android:layout_above="@+id/onOffSwitch"
        android:layout_alignParentEnd="true" />

    <SeekBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/volume1"
        android:max="100"
        android:layout_above="@+id/reverb1"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="23dp" />

    <TextView
        android:text="Reverb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_above="@+id/reverb1"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:layout_above="@+id/volume1"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="G#m"
        android:layout_width="40dp"
        android:layout_height="70dp"
        android:id="@+id/chordGsharpdim"
        style="@android:style/Widget.Holo.Light.Button.Small"
        android:layout_above="@+id/textView7"
        android:layout_toEndOf="@+id/keyD" />

    <Button
        android:text="F#m"
        android:layout_width="40dp"
        android:layout_height="70dp"
        android:id="@+id/chordFsharpdim"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignTop="@+id/chordGsharpdim"
        android:layout_toEndOf="@+id/textView6" />

    <Button
        android:text="E"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/chordE"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignTop="@+id/chordFsharpdim"
        android:layout_alignParentStart="true" />

    <Button
        android:text="D#m"
        android:layout_width="40dp"
        android:layout_height="70dp"
        android:id="@+id/chordDsharpdim"
        style="@android:style/Widget.Holo.Light.Button.Small"
        android:layout_alignTop="@+id/chordCsharpdim"
        android:layout_alignStart="@+id/noteDsharp" />

    <ToggleButton
        android:text="ToggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/onOffSwitch"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="B"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/chordB"
        style="@android:style/Widget.Holo.Light.Button.Small"
        android:layout_alignTop="@+id/chordA"
        android:layout_alignStart="@+id/noteB" />

    <TextView
        android:text="Chord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_above="@+id/chordFsharpdim"
        android:layout_alignStart="@+id/textView4" />

    <Button
        android:text="Higher"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:id="@+id/octaveHigher"
        android:layout_alignTop="@+id/octaveLower"
        android:layout_alignEnd="@+id/keyA" />

    <Button
        android:text="G"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyG"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/keyD"
        android:layout_alignBottom="@+id/keyD"
        android:layout_toEndOf="@+id/onOffSwitch" />

    <Button
        android:text="A"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyA"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/keyD"
        android:layout_alignBottom="@+id/keyD"
        android:layout_toStartOf="@+id/noteDsharp" />

    <Button
        android:text="B"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyB"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/keyD"
        android:layout_alignBottom="@+id/keyD"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="F"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyF"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/keyD"
        android:layout_alignBottom="@+id/keyD"
        android:layout_alignEnd="@+id/textView5" />

    <Button
        android:text="C"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyC"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/keyD"
        android:layout_alignBottom="@+id/keyD"
        android:layout_alignParentStart="true" />

    <Button
        android:text="D"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyD"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_below="@+id/textView"
        android:layout_toEndOf="@+id/textView3" />

    <Button
        android:text="A"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/chordA"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignTop="@+id/chordGsharpdim"
        android:layout_toEndOf="@+id/chordGsharpdim" />

    <TextView
        android:text="Octave"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:id="@+id/textView3"
        android:layout_below="@+id/keyD"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Lower"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:id="@+id/octaveLower"
        android:layout_below="@+id/textView3"
        android:layout_toEndOf="@+id/keyC" />

    <Button
        android:text="G#"
        android:layout_width="40dp"
        android:layout_height="60dp"
        android:id="@+id/noteGsharp"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/noteCsharp"
        android:layout_alignBottom="@+id/noteCsharp"
        android:layout_toEndOf="@+id/noteFsharp" />

    <Button
        android:text="E"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/noteE"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/noteGsharp"
        android:layout_alignBottom="@+id/noteGsharp"
        android:layout_alignParentStart="true" />

    <Button
        android:text="A"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/noteA"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignTop="@+id/noteFsharp"
        android:layout_toEndOf="@+id/noteGsharp" />

    <Button
        android:text="B"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/noteB"
        style="@android:style/Widget.Holo.Light.Button.Small"
        android:layout_alignTop="@+id/noteE"
        android:layout_toEndOf="@+id/keyF" />

    <Button
        android:text="E"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/keyE"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/keyF"
        android:layout_alignBottom="@+id/keyF"
        android:layout_toStartOf="@+id/keyF" />

    <Button
        android:text="F#"
        android:layout_width="40dp"
        android:layout_height="60dp"
        android:id="@+id/noteFsharp"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_above="@+id/textView6"
        android:layout_toEndOf="@+id/textView4" />

    <TextView
        android:text="Note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_below="@+id/octaveLower"
        android:layout_alignParentStart="true" />

    <Button
        android:text="D#"
        android:layout_width="40dp"
        android:layout_height="60dp"
        android:id="@+id/noteDsharp"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignTop="@+id/noteA"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="C#"
        android:layout_width="40dp"
        android:layout_height="60dp"
        android:id="@+id/noteCsharp"
        style="@android:style/Widget.DeviceDefault.Light.Button.Small"
        android:layout_alignBaseline="@+id/noteDsharp"
        android:layout_alignBottom="@+id/noteDsharp"
        android:layout_alignStart="@+id/text" />

    <Button
        android:text="C#m"
        android:layout_width="40dp"
        android:layout_height="70dp"
        android:id="@+id/chordCsharpdim"
        style="@android:style/Widget.Holo.Light.Button.Small"
        android:layout_alignBaseline="@+id/chordA"
        android:layout_alignBottom="@+id/chordA"
        android:layout_toEndOf="@+id/chordB" />

</RelativeLayout>

